{
  "root": "true",
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "node": true
  },
  "rules": {
    "comma-dangle": 0,
    "strict": ["off", "safe"],
    "no-confusing-arrow": 0,
    "no-nested-ternary": 0,
    "max-len": 0,
    "no-multi-str": 0,
    "func-names": 0,
    "no-unused-vars": 0,
    "prefer-arrow-callback": 0,
    "no-console": 0
  }
}
